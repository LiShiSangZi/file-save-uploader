{
  "name": "file-save-uploader",
  "displayName": "file-save-uploader",
  "description": "SCP a file to the remote server once the file is saved.",
  "version": "0.0.8",
  "publisher": "gogocrow",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.6.0",
    "node": ">=6.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "gogocrow.uploadAll",
        "title": "Upload Whole Workspace"
      }
    ],
    "keybindings": [
      {
        "mac": "cmd+alt+u",
        "win": "ctrl+alt+u",
        "linux": "ctrl+alt+u",
        "key": "ctrl+alt+u",
        "command": "gogocrow.uploadAll"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LiShiSangZi/file-save-uploader"
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "eslint": "^3.6.0",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "co": "^4.6.0",
    "rsync": "^0.6.1"
  }
}
